    <br>

    <div class="container">
        <form action="/crates/serendia" method="POST">
            <label>Craft Count </label><input type="number" name="crafts" value="<%= data.user.crafts %>" min="1"><br>
            <label>Processing Average </label><input type="number" name="processingAvg" value="<%= data.user.processingAvg %>" max="3" min="0.01" step="0.01"><br>
            <label>Processing Proc Average</label><input type="number" name="processingProcAvg" value="<%= data.user.processingProcAvg %>" max="1.5" min="0.01" step="0.01"><br>
            <button type="submit">Calculate</button>
    </div>

    <br>

    <div class="container">
        <div class="row">

            <!-- Begin left table -->

            <div class="col-sm">
                <table class="table">
                    <thead>
                        <tr class="d-flex">
                            <th scope="col" class="col-2">Count</th>
                            <th scope="col" class="col-5">Ingredient</th>
                            <th scope="col" class="col-2">Cost</th>
                            <th scope="col" class="col-3">Total Cost</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="d-flex">
                            <th scope="row" class="col-2">
                                <input id="bspCount" type="number" class="col-12" value="<%= data.materials.bsp %>" disabled>
                            </th>
                            <td class="col-5">Black Stone Powder</td>
                            <td scope="row" class="col-2">
                                <input name="bspCost" type="number" class="col-12" value="<%= data.materials.bspCost %>">
                            </td>
                            <td scope="row" class="col-3">
                                <%= data.materials.bspBatchPretty %>
                            </td>
                        </tr>
                        <tr class="d-flex">
                            <th scope="row" class="col-2">
                                <input id="timberCount1" type="number" class="col-12" value="<%= data.materials.timber %>" disabled>
                            </th>
                            <td class="col-5">Maple Timber</td>
                            <td class="col-2">
                                <input name="timber1Cost" type="number" class="col-12" value="<%= data.materials.timber1Cost %>">
                            </td>
                            <td class="col-3">
                                <%= data.materials.timber1BatchPretty %>
                            </td>
                        </tr>
                        <tr class="d-flex">
                            <th scope="row" class="col-2">
                                <input id="timberCount2" type="number" class="col-12" value="<%= data.materials.timber %>" disabled>
                            </th>
                            <td class="col-5">Pine Timber</td>
                            <td class="col-2">
                                <input name="timber2Cost" type="number" class="col-12" value="<%= data.materials.timber2Cost %>">
                            </td>
                            <td class="col-3">
                                <%= data.materials.timber2BatchPretty %>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>

            <!-- Begin right table -->

            <div class="col-sm">
                <table class="table">
                    <thead>
                        <tr class="d-flex">
                            <th scope="col" class="col-2">Count</th>
                            <th scope="col" class="col-5">Ingredient</th>
                            <th scope="col" class="col-2">Value</th>
                            <th scope="col" class="col-3 text-end">Batch Value</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="d-flex">
                            <th scope="row" class="col-2">
                                <input id="crateCount" type="number" class="col-12" value="<%= data.crate.count %>" disabled>
                            </th>
                            <td class="col-5"><%= data.crate.name %> Timber Crate</td>
                            <td class="col-2">
                                <input id="crateValue" type="number" class="col-12" value="<%= data.crate.value %>" disabled>
                            <td>
                            <td id="crateBatch" class="col-3 text-end"><%= data.crate.batchPretty %></td>
                        </tr>

                        <!-- 
    
    
                            TODO PLYWOOD
    
    
                        -->
                        <tr class="d-flex">
                            <th id="distanceCount" scope="row" class="col-2"><%= data.distance.countPretty %></th>
                            <td class="col-5">

                                <select name="distance" class="form-control form-control-sm">
                                    <option value="113.85" selected>Grana -> Valencia</option>
                                    <option value="128.6">Grana -> Arehaza</option>
                                </select>

                            </td>
                            <td id="distanceValue" class="col-2"><%= data.distance.valuePretty %></td>
                            <td id="distanceBatch" class="col-3 text-end"><%= data.distance.batchPretty %></td>
                        </tr>
                        <%- include("_form_bargain") %>
                        <tr class="d-flex">
                            <th id="desertCount" scope="row" class="col-2"><%= data.desert.countPretty %></th>
                            <td class="col-5">

                                <label class="form-check-label" for="desertBonus">
                                    Desert Bonus:
                                </label>
                                <input name="desertStatus" class="form-check-input" type="checkbox" checked>

                            </td>
                            <td id="desertValue" class="col-2"><%= data.desert.valuePretty %></td>
                            <td id="desertBatch" class="col-3 text-end"><%= data.desert.batchPretty %></td>
                        </tr>
                        <tr class="d-flex">
                            <th scope="row" class="col-2"></th>
                            <td class="col-5">Trade Value</td>
                            <td id="ingredientsCost" class="col-2"><%= data.profit.valuePretty %></td>
                            <td id="ingredientsBatch" class="col-3 text-end"><%= data.profit.valueBatchPretty %></td>
                        </tr>
                        <tr class="d-flex">
                            <th scope="row" class="col-2"></th>
                            <td class="col-5">Ingredients Cost</td>
                            <td id="ingredientsCost" class="col-2"><%= data.profit.costPretty %></td>
                            <td id="ingredientsBatch" class="col-3 text-end"><%= data.profit.costBatchPretty %></td>
                        </tr>
                        <tr class="d-flex">
                            <th scope="row" class="col-2"></th>
                            <td class="col-5">Total Profit</td>
                            <td id="profit" class="col-2"><%= data.profit.profitPretty %></td>
                            <td id="batchProfit" class="col-3 text-end"><%= data.profit.profitBatchPretty %></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    </form>

    <!-- Begin Material List -->
    <!-- end of document -->


    <p><%= data.user.crafts %></p>