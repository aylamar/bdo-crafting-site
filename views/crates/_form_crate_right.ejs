<tr class="d-flex">
    <th id="distanceCount" scope="row" class="col-2"><%= data.distance.countPretty %></th>
    <td class="col-5">

        <select name="distance" class="form-control form-control-sm">
            <option value="113.85">Grana -> Valencia</option>
            <option value="128.6">Grana -> Arehaza</option>
        </select>

    </td>
    <td id="distanceValue" class="col-2"><%= data.distance.valuePretty %></td>
    <td id="distanceBatch" class="col-3"><%= data.distance.batchPretty %></td>
</tr>
<tr class="d-flex">
    <th id="bargainCount" scope="row" class="col-2"><%= data.bargain.valuePretty %></th>
    <td class="col-5">

        <select name="bargain" class="form-control form-control-sm"">
            <option value="0.255">Artisan 1</option>
            <option value="0.38">Master 16</option>
            <option value="0.385">Master 17</option>
        </select>

    </td>
    <td id="bargainValue" class="col-2"><%= data.bargain.valuePretty %></td>
    <td id="bargainBatch" class="col-3"><%= data.bargain.valuePretty %></td>
</tr>
<tr class="d-flex">
    <th id="desertCount" scope="row" class="col-2"><%= data.desert.countPretty %></th>
    <td class="col-5">

        <label class="form-check-label" for="desertBonus">
            Desert Bonus:
        </label>
        <input name="desertStatus" class="form-check-input" type="checkbox" checked>

    </td>
    <td id="desertValue" class="col-2"><%= data.desert.valuePretty %></td>
    <td id="desertBatch" class="col-3"><%= data.desert.batchPretty %></td>
</tr>