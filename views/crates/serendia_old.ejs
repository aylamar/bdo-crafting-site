<head>
    <title>Serendia Crates</title>
</head>

<br>

<div class="container">
    <div class="row">
        <div class="col-sm">
            <label>Craft Count</label><input type="number" id="craftCount" value="1" min="0"><br>
            <label>Processing Average </label><input type="number" id="processingAverage" value="2.5" min="0" step="0.01"><br>
            <label>Processing Proc Average</label><input type="number" id="processingProcAverage" value="0.05" min="0" step="0.01"><br>
            <button type="submit" onclick="calculate()">Calculate</button>        </div>
        <div class="col-sm">

        </div>
    </div>
</div>

<br>

<div class="container">
    <div class="row">

        <!-- Begin left table -->

        <div class="col-sm">
            <table class="table">
                <thead>
                    <tr class="d-flex">
                        <th scope="col" class="col-2">Count</th>
                        <th scope="col" class="col-5">Ingredient</th>
                        <th scope="col" class="col-2">Cost</th>
                        <th scope="col" class="col-3">Total Cost</th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="d-flex">
                        <th scope="row" class="col-2">
                            <input id="bspCount" type="number" class="col-12" value="1" disabled>
                        </th>
                        <td class="col-5">Black Stone Powder</td>
                        <td scope="row" class="col-2">
                            <input id="bspCost" type="number" class="col-12" value="<%= var0 %>">
                        </td>
                        <td scope="row" class="col-3">
                            <span id="bspBatch"><%= var0 %></span>
                        </td>
                    </tr>
                    <tr class="d-flex">
                        <th scope="row" class="col-2">
                            <input id="timberCount1" type="number" class="col-12" value="40" disabled>
                        </th>
                        <td class="col-5">Maple Timber</td>
                        <td class="col-2">
                            <input id="timberPrice1" type="number" class="col-12" value="<%= var1 %>">
                        </td>
                        <td class="col-3">
                            <span id="timberBatch1"><%= var1*40 %></span>
                        </td>
                    </tr>
                    <tr class="d-flex">
                        <th scope="row" class="col-2">
                            <input id="timberCount2" type="number" class="col-12" value="40" disabled>
                        </th>
                        <td class="col-5">Pine Timber</td>
                        <td class="col-2">
                            <input id="timberPrice2" type="number" class="col-12" value="<%= var2 %>">
                        </td>
                        <td class="col-3">
                            <span id="timberBatch2"><%= var2*40 %></span>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>

        <!-- Begin right table -->

        <div class="col-sm">
            <table class="table">
                <thead>
                    <tr class="d-flex">
                        <th scope="col" class="col-2">Count</th>
                        <th scope="col" class="col-5">Ingredient</th>
                        <th scope="col" class="col-2">Value</th>
                        <th scope="col" class="col-3">Batch Value</th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="d-flex">
                        <th scope="row" class="col-2">
                            <input id="crateCount" type="number" class="col-12" value="1" disabled>
                        </th>
                        <td class="col-5">Serendia Timber Crate</td>
                        <td class="col-2"><input id="crateValue" type="number" class="col-12" value="32550" disabled>
                        <td>
                        <td id="crateBatch" class="col-3">32,550</td>
                    </tr>

                    <!-- 


                        TODO PLYWOOD


                    -->

                    <tr class="d-flex">
                        <th id="distanceCount" scope="row" class="col-2">113.85%</th>
                        <td class="col-5">
                    
                            <select id="distance" class="form-control form-control-sm">
                                <option value="113.85">Grana -> Valencia</option>
                                <option value="128.6">Grana -> Arehaza</option>
                            </select>
                    
                        </td>
                        <td id="distanceValue" class="col-2">37,058</td>
                        <td id="distanceBatch" class="col-3">0</td>
                    </tr>
                    <tr class="d-flex">
                        <th id="bargainCount" scope="row" class="col-2">38%</th>
                        <td class="col-5">
                    
                            <select id="bargain" class="form-control form-control-sm"">
                                <option value="0.255">Artisan 1</option>
                                <option value="0.38">Master 16</option>
                                <option value="0.385">Master 17</option>
                            </select>
                    
                        </td>
                        <td id="bargainValue" class="col-2">32,550</td>
                        <td id="bargainBatch" class="col-3">0</td>
                    </tr>
                    <tr class="d-flex">
                        <th id="desertCount" scope="row" class="col-2">50%</th>
                        <td class="col-5">
                    
                            <label class="form-check-label" for="desertBonus">
                                Desert Bonus:
                            </label>
                            <input id="desertStatus" class="form-check-input" type="checkbox" checked>
                    
                        </td>
                        <td id="desertValue" class="col-2">32,550</td>
                        <td id="desertBatch" class="col-3">0</td>
                    </tr>

                    <tr class="d-flex">
                        <th scope="row" class="col-2"></th>
                        <td class="col-5">Ingredients Cost</td>
                        <td id="ingredientsCost" class="col-2">0</td>
                        <td id="ingredientsBatch" class="col-3">0</td>
                    </tr>
                    <tr class="d-flex">
                        <th scope="row" class="col-2"></th>
                        <td class="col-5">Total Profit</td>
                        <td id="profit" class="col-2">0</td>
                        <td id="batchProfit" class="col-3">0</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

    <!-- Begin Material List -->


    <div class="container">
        <div class="row">
            <div class="col-3 m-1">
                <span class="bg-secondary text-white p-2 rounded">10 Maple Plywood
                    <input id="plywood1" type="number" class="col-3" value="5" disabled></span>
            </div>
        </div>
        <div class="row">
            <div class="col-1 m-1"></div>
            <div class="col-3 m-1">
                <span class="bg-secondary text-white p-2 rounded">10 Maple Plank
                    <input id="plank1" type="number" class="col-3" value="5" disabled></span>
            </div>
        </div>
        <div class="row">
            <div class="col-2 m-1"></div>
            <div class="col-3 m-1">
                <span class="bg-secondary text-white p-2 rounded">10 Maple Timber
                    <input id="timber1" type="number" class="col-3" value="5" disabled></span>
            </div>
        </div>
        <div class="row">
            <div class="col-3 m-1">
                <span class="bg-secondary text-white p-2 rounded">10 Pine Plywood
                    <input id="plywood2" type="number" class="col-3" value="5" disabled></span>
            </div>
        </div>
        <div class="row">
            <div class="col-1 m-1"></div>
            <div class="col-3 m-1">
                <span class="bg-secondary text-white p-2 rounded">10 Pine Plank
                    <input id="plank2" type="number" class="col-3" value="5" disabled></span>
            </div>
        </div>
        <div class="row">
            <div class="col-2 m-1"></div>
            <div class="col-3 m-1">
                <span class="bg-secondary text-white p-2 rounded">10 Pine Timber
                    <input id="timber2" type="number" class="col-3" value="5" disabled></span>
            </div>
        </div>
        <div class="row">
            <div class="col-3 m-1">
                <span class="bg-secondary text-white p-2 rounded">10 Black Stone Powder
                    <input id="bsp" type="number" class="col-3" value="5" disabled></span>
            </div>
        </div>
    </div>

    <!-- end of document -->

</div>


<script src="/js/crate_calc.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js" integrity="sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0" crossorigin="anonymous"></script>